geom_boxplot(aes(fill = EmploymentStatus), show.legend = F) +
theme(axis.text.x = element_text(angle = 90))+
xlab("Employment Status") +
ylab("Borrower Rate")
ggplot(aes(x = Term, y = LoanOriginalAmount), data = loan_data) +
geom_boxplot(aes(fill = EmploymentStatus), show.legend = F) +
xlab("Employment Status") +
ylab("Borrower Rate")
ggplot(aes(x = Term.f, y = LoanOriginalAmount), data = loan_data) +
geom_boxplot(aes(fill = EmploymentStatus), show.legend = F) +
xlab("Employment Status") +
ylab("Borrower Rate")
ggplot(aes(x = LoanOriginationDate.year, y =DollarTotal ),
data =  subset(loan_data, !is.na(DebtToIncomeRatio.bucket))) +
geom_histogram()+
facet_wrap(~IncomeRange)
